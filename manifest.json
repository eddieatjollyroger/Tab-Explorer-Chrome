{
  "manifest_version": 3,
  "name": "Tab Explorer",
  "version": "3",
  "background": {
  "service_worker": "js/background.js"
},
  "description": "New tab page showing tabs as a file explorer",
  "permissions": [
    "tabs",
    "topSites",
    "search",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
